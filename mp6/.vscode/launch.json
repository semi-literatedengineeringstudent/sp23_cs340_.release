{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run App",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "debug",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run",
            ],
        },
    ],
}